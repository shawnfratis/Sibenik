procedure scene01(b)

(ax := b[1],bx := b[2],cx := b[3])
(dx := b[4],ex := b[5],fx := b[6])
(gx := b[7],hx := b[8],ix := b[9])
(ss := b[10],jq := b[11])

cc := colorTable(jq)


   (xc := 0,yc := 0)

   i := (dx||ex||fx)   #15

   size := (ax||bx||cx)  #was 10
   radius := size / .8
   xc := yc := size / .8

   angle := 0.0
   incr := (gx||hx||ix) * &pi / i

   every j := 1 to i do {
      spokes(xc + radius * cos(angle), yc + radius * sin(angle),
      radius, i, angle,ss,cc)
      angle +:= incr
      }
 
end

procedure spokes(x, y, r, i, angle,s,c)
   local incr, j

   incr := 2 * &pi / i 
  Fg(c)
   every j := 1 to i do {
DrawSphere(x + r * cos(angle),0,y + r * sin(angle), s)

      angle +:= incr
      }

   return

#radii..use x,y,r
   
end

