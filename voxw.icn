procedure texcubein(at)

(ax := at[1],bx := at[2],cx := at[3])
(dx := at[4],ex := at[5],fx := at[6])
(gx := at[7],hx := at[8],ix := at[9])

x := (ax||bx||cx)  
y := (dx||ex||fx) 
z := (gx||hx||ix) 

tpcubemain01(x,y,z)

end

procedure tpcubemain01(xa1,ya1,za1)

xa2 := xa1
ya2 := ya1
za2 := (za1 + 1)

xa3 := xa1
ya3 := (ya1 - 1)
za3 := (za1 + 1)

xa4 := xa1
ya4 := (ya1 - 1)
za4 := za1

xb2 := (xa1 + 1)
yb2 := ya1
zb2 := za1

xb3 := (xa1 + 1)
yb3 := ya1
zb3 := (za1 + 1)

xb4 := (xa1 + 1)
yb4 := (ya1 - 1)
zb4 := (za1 + 1)

#vals that work

#FillPolygon(0,2,0,0,2,1,0,1,1,0,1,0)

#FillPolygon(0,2,0,1,2,0,1,2,1,0,2,1)

#FillPolygon(0,2,1,1,2,1,1,1,1,0,1,1)

tcplane_a(xa1,ya1,za1,xa2,ya2,za2,xa3,ya3,za3,xa4,ya4,za4)

tcplane_b(xa1,ya1,za1,xb2,yb2,zb2,xb3,yb3,zb3,xa2,ya2,za2)

tcplane_c(xa2,ya2,za2,xb3,yb3,zb3,xb4,yb4,zb4,xa3,ya3,za3) 

tcplane_d(xb3,ya1,za1,xb3,ya2,za2,xb3,ya3,za3,xb3,ya4,za4) 

end


