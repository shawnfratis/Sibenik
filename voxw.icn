procedure cube01(at)

(ax := at[1],bx := at[2],cx := at[3])
(dx := at[4],ex := at[5],fx := at[6])
(gx := at[7],hx := at[8],ix := at[9])
(jx := at[10],jq := at[11])

c := colorTable(jq)

x := (ax||bx||cx) 
y := (dx||ex||fx) 
z := (gx||hx||ix) 
s := jx

inpglcube01(x,y,z,s,c)

return()

end

